let twotaglh = document.getElementById("twotaglh");
let twotaghl = document.getElementById("twotaghl");


twotaghl.onclick = () => {
    let arr = data.sort((a, b) => (a.newPrice < b.newPrice ? 1 : -1));

    showProducts(arr);
}

  // sort low to high

twotaglh.onclick = () => {
    let arr = data.sort((a, b) => (a.newPrice > b.newPrice ? 1 : -1));

